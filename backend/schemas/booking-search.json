{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "bookingCode": {
            "type": "string",
            "pattern": "^[A-Z0-9]{6}$"
        },
        "familyName": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "bookingCode",
        "familyName"
    ]
}